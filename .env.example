# Database Configuration
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/siprod
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password
POSTGRES_DB=siprod
POSTGRES_PORT=5432

# Backend Configuration
JWT_SECRET=your_jwt_secret
PORT=4000
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
API_PREFIX=/api

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://127.0.0.1:4000/api
NEXT_TELEMETRY_DISABLED=1
FRONTEND_PORT=3000

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PORT=6379

# CORS Configuration
CORS_ORIGIN=http://localhost:3000

# Environment
NODE_ENV=development

# Production Values (commented out)
# NEXT_PUBLIC_API_URL=http://api.siprod.gob.ar/api
# CORS_ORIGIN=https://siprod.gob.ar
# DATABASE_URL=postgresql://postgres:your_password@db:5432/siprod
